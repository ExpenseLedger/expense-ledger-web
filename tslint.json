{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "*.json",
      "src/**/*.json"
    ]
  },
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "interface-name": false
  }
}